<!DOCTYPE html>
<html>
	<head>
  		<title>Lighthouseapp</title>
  		<%= stylesheet_link_tag    "application", :media => "all" %>
  		<%= javascript_include_tag "application" %>
  		<%= csrf_meta_tags %>
		<%= yield :head %>
	</head>

	<body>

		<% if user_signed_in? %>
			<%= link_to 'Sign Out', destroy_user_session_path, method: :delete %>
		<% end %>

		<%= yield %>

	</body>
</html>
